{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Capability for the all windows",
    "windows": [
        "select",
        "config",
        "overlay"
    ],
    "permissions": [
        "core:default",
        "shell:allow-open",
        "process:default",
        "core:window:allow-close",
        "core:window:allow-hide",
        "core:window:allow-show"
    ]
}